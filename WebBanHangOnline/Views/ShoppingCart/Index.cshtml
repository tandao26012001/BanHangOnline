
@using WebBanHangOnline.Common
@{
    ViewBag.Title = "Giỏ hàng";
}

<!-- breadcrumb-section -->
<div class="breadcrumb-section breadcrumb-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 offset-lg-2 text-center">
                <div class="breadcrumb-text">
                    <p>@ViewBag.Title</p>
                    <h1>Chi tiết giỏ hàng</h1>
